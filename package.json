{
	"name": "Holler-Elementor",
	"version": "2.1.7",
	"description": "Gulpfile setup for Javascript ES6 compiling, plus SCSS, images, and fonts handling",
	"author": "James Murgatroyd <hello@hollerdigital.com> (https://hollerdigital.com)",
	"repository": {
		"type": "git",
		"url": "https://github.com/HollerDigital/holler-elementor"
	},
	"keywords": [
		"gulp",
		"es6",
		"scss"
	],
	"dependencies": {
		"semver": "^7.5.4"
	},
	"devDependencies": {
		"@babel/core": "latest",
		"@babel/preset-env": "latest",
		"babelify": "latest",
		"browser-sync": "latest",
		"browserify": "latest",
		"browserify-shim": "latest",
		"graceful-fs": "latest",
		"del": "latest",
		"gulp": "latest",
		"gulp-autoprefixer":"latest",
		"gulp-concat": "latest",
		"gulp-line-ending-corrector": "latest",
		"gulp-merge-media-queries": "latest",
		"gulp-notify": "latest",
		"gulp-options": "latest",
		"gulp-plumber": "latest",
		"gulp-rename": "latest",
		"gulp-sass": "latest",
		"gulp-sourcemaps": "latest",
		"gulp-strip-debug":"latest",
		"gulp-uglify": "latest",
		"gulp-uglifycss": "latest",
		"node-sass": "latest",
		"vinyl-buffer": "latest",
		"vinyl-source-stream": "latest"
	},
	"babel": {
		"presets": [
			"@babel/preset-env"
		]
	},
	"browserify": {
		"transform": [
			"browserify-shim"
		]
	},
	"browser": {
		"jquery": "./node_modules/jquery/dist/jquery.js"
	},
	"browserify-shim": {
		"jquery": "$"
	},
	"license": "GPL-3.0"
}
